FPP: specpp  -DSPEC_CPU_NO_HOLLERITH -DSPEC_CPU -DNDEBUG -DSPEC_CPU_LP64   <source> -o <source>.fppized.f9x
P: BENCH_FPPFLAGS="-DSPEC_CPU_NO_HOLLERITH"
P: CPUFLAGS="-DSPEC_CPU -DNDEBUG"
P: PORTABILITY="-DSPEC_CPU_LP64"
P: FPORTABILITY=""
COMP: /usr/bin/gfortran -c -o options.o    -O2       -DSPEC_CPU_LP64 -funconstrained-commons -std=legacy        <source>
C: FC="/usr/bin/gfortran"
C: FOBJOPT="-c -o options"
P: CPUFLAGS="-DSPEC_CPU -DNDEBUG"
P: BENCH_FLAGS=""
P: BENCH_FFLAGS=""
O: OPTIMIZE=""
O: FOPTIMIZE="-O2"
P: PORTABILITY="-DSPEC_CPU_LP64"
P: FPORTABILITY="-funconstrained-commons -std=legacy"
O: EXTRA_FFLAGS=""
O: EXTRA_OPTIMIZE=""
O: EXTRA_FOPTIMIZE=""
P: EXTRA_PORTABILITY=""
P: EXTRA_FPORTABILITY=""
LINK: /usr/bin/gfortran   -O2  -DSPEC_CPU_LP64 -funconstrained-commons -std=legacy       <objects>   -lm        -o options
C: LD="/usr/bin/gfortran"
O: FOPTIMIZE="-O2"
P: PORTABILITY="-DSPEC_CPU_LP64"
P: FPORTABILITY="-funconstrained-commons -std=legacy"
C: MATH_LIBS="-lm"
C: LDOUT="-o options"
